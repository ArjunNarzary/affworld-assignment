{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/index.ts"
    },
    {
      "source": "/upload/(.*)",
      "destination": "/public/upload/$1"
    }
  ],
  "public": "public"
}
